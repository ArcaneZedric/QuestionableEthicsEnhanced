<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <Operation Class="PatchOperationConditional">
    <xpath>Defs/HediffDef[defName="WitcherTrials_Hediff_Grasses"]</xpath>
    <success>Always</success>
    <match Class="PatchOperationAddModExtension">
      <xpath>Defs/HediffDef[defName="WitcherTrials_Hediff_Grasses" or defName="WitcherTrials_Hediff_Dreams" or defName="WitcherTrials_Hediff_Elders"]</xpath>
      <value>
        <li Class="QEthics.HediffTemplateProperties">
          <includeInGenomeTemplate>true</includeInGenomeTemplate>
          <includeInBrainTemplate>false</includeInBrainTemplate>
        </li>
      </value>
    </match>
  </Operation>
</Patch>
